{
    "compilerOptions": {
        "module": "ES6",
        "target": "es6",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "lib": ["es6", "DOM"],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceMap": true,
        "declaration": true
    }
}
